# Local Development Environment for ScooterShare Pro
FLASK_APP=run.py
FLASK_ENV=development

# Security
SECRET_KEY=dev-secret-key-change-in-production
JWT_SECRET_KEY=jwt-secret-key-change-in-production

# Database (Docker)
DATABASE_URL=postgresql://scooter_user:scooter_pass@localhost:5432/scooter_share_pro

# Pricing
BASE_PRICE_PER_MINUTE=0.25
START_FEE=1.00

# Limits
MAX_RENTAL_TIME_HOURS=24
